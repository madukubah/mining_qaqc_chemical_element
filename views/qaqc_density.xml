<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="qaqc_density_list" model="ir.ui.view">
        <field name="name">qaqc.density.list</field>
        <field name="model">qaqc.density</field>
        <field name="type">tree</field>
        <field name="priority" eval="8"/>
        <field name="arch" type="xml">
            <tree string="Ore Specification List">
                <field name="name" />
                <field name="density" />
            </tree>
        </field>
    </record>

    <record id="view_qaqc_density_form" model="ir.ui.view">
            <field name="name">qaqc.density.form</field>
            <field name="model">qaqc.density</field>
            <field name="priority">1</field>
            <field name="arch" type="xml">
                <form string="Ore Specification">
                    <sheet>
                        <div class="oe_title">
                            <label for="name" class="oe_edit_only" string="Name" ></label>
                            <h1>
                                <field name="name" />
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="density" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
    </record>
</odoo>
